{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "filament/spatie-laravel-media-library-plugin": "^5.0",
        "laravel/framework": "^12.47.0",
        "laravel/tinker": "^2.11.0",
        "netserva/cms": "@dev",
        "netserva/core": "@dev",
        "netserva/dns": "@dev",
        "netserva/fleet": "@dev",
        "netserva/mail": "@dev",
        "netserva/crm": "@dev",
        "netserva/db": "@dev",
        "netserva/platform": "@dev",
        "netserva/web": "@dev",
        "phpseclib/phpseclib": "^3.0.48"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "laravel/boost": "^1.8.9",
        "laravel/pail": "^1.2.4",
        "laravel/pint": "^1.27.0",
        "laravel/sail": "^1.52",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "pestphp/pest": "^4.3.1@dev",
        "pestphp/pest-plugin-browser": "^4.2.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "NetServa\\Cms\\Database\\Factories\\": "packages/netserva-cms/database/factories/",
            "NetServa\\Core\\Database\\Factories\\": "packages/netserva-core/database/factories/",
            "NetServa\\Dns\\Database\\Factories\\": "packages/netserva-dns/database/factories/",
            "NetServa\\Fleet\\Database\\Factories\\": "packages/netserva-fleet/database/factories/",
            "NetServa\\Mail\\Database\\Factories\\": "packages/netserva-mail/database/factories/",
            "NetServa\\Web\\Database\\Factories\\": "packages/netserva-web/database/factories/",
            "NetServa\\Crm\\Database\\Factories\\": "packages/netserva-crm/database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "trap 'kill 0' EXIT; php artisan serve & bun run dev & wait"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "backup": [
            "./scripts/backup-db.sh"
        ],
        "restore": [
            "./scripts/restore-db.sh"
        ],
        "safe-fresh": [
            "./scripts/safe-migrate-fresh.sh"
        ],
        "db:fresh": [
            "./scripts/backup-db.sh pre-fresh",
            "@php artisan migrate:fresh"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "pest": {
            "type": "vcs",
            "url": "https://github.com/pestphp/pest.git"
        },
        "netserva/cms": {
            "type": "path",
            "url": "./packages/netserva-cms",
            "options": {
                "versions": {
                    "netserva/cms": "0.0.3"
                }
            }
        },
        "netserva/dns": {
            "type": "path",
            "url": "./packages/netserva-dns",
            "options": {
                "versions": {
                    "netserva/dns": "0.0.3"
                }
            }
        },
        "netserva/mail": {
            "type": "path",
            "url": "./packages/netserva-mail",
            "options": {
                "versions": {
                    "netserva/mail": "0.0.3"
                }
            }
        },
        "netserva/web": {
            "type": "path",
            "url": "./packages/netserva-web",
            "options": {
                "versions": {
                    "netserva/web": "0.0.3"
                }
            }
        },
        "netserva/core": {
            "type": "path",
            "url": "./packages/netserva-core",
            "options": {
                "versions": {
                    "netserva/core": "0.0.3"
                }
            }
        },
        "netserva/fleet": {
            "type": "path",
            "url": "./packages/netserva-fleet",
            "options": {
                "versions": {
                    "netserva/fleet": "0.0.3"
                }
            }
        },
        "netserva/platform": {
            "type": "path",
            "url": "./packages/netserva-platform",
            "options": {
                "versions": {
                    "netserva/platform": "0.0.3"
                }
            }
        },
        "netserva/crm": {
            "type": "path",
            "url": "./packages/netserva-crm",
            "options": {
                "versions": {
                    "netserva/crm": "0.0.1"
                }
            }
        },
        "netserva/db": {
            "type": "path",
            "url": "./packages/netserva-db",
            "options": {
                "versions": {
                    "netserva/db": "1.0.0"
                }
            }
        }
    }
}
