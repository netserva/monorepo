{
    "name": "netserva/ops",
    "description": "NetServa Operations - Centralized monitoring, backup management, analytics, and observability platform for infrastructure operations",
    "type": "library",
    "license": "MIT",
    "keywords": ["netserva", "monitoring", "backup", "analytics", "observability", "operations", "laravel", "filament"],
    "homepage": "https://netserva.org",
    "authors": [
        {
            "name": "Mark Constable",
            "email": "markc@netserva.org",
            "homepage": "https://netserva.org"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "filament/filament": "^4.0",
        "netserva/core": "*",
        "symfony/process": "^7.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NetServa\\Ops\\": "src/",
            "NetServa\\Ops\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetServa\\Ops\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetServa\\Ops\\NetServaOpsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}