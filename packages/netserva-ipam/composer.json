{
    "name": "netserva/ipam",
    "description": "NetServa IP Address Management - Comprehensive IPAM solution for network planning, IP allocation, and subnet management",
    "type": "library",
    "license": "MIT",
    "keywords": ["netserva", "ipam", "networking", "ip-management", "subnets", "laravel", "filament"],
    "homepage": "https://netserva.org",
    "authors": [
        {
            "name": "Mark Constable",
            "email": "markc@netserva.org",
            "homepage": "https://netserva.org"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "filament/filament": "^4.0",
        "netserva/core": "*"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NetServa\\Ipam\\": "src/",
            "NetServa\\Ipam\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetServa\\Ipam\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetServa\\Ipam\\NetServaIpamServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}