{
    "name": "netserva/config",
    "description": "NetServa Configuration & Secrets - Unified configuration management, secrets handling, and database orchestration for infrastructure components",
    "type": "library",
    "license": "MIT",
    "keywords": ["netserva", "configuration", "secrets", "database", "management", "laravel", "filament"],
    "homepage": "https://netserva.org",
    "authors": [
        {
            "name": "Mark Constable",
            "email": "markc@netserva.org",
            "homepage": "https://netserva.org"
        }
    ],
    "support": {
        "issues": "https://github.com/netserva/monorepo/issues",
        "source": "https://github.com/netserva/monorepo"
    },
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "filament/filament": "^4.0",
        "netserva/core": "^0.0.3",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NetServa\\Config\\": "src/",
            "NetServa\\Config\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetServa\\Config\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetServa\\Config\\NetServaConfigServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}