{
    "name": "netserva/cron",
    "description": "NetServa Automation & Scheduling - Task orchestration, cron job management, and automation workflows for infrastructure operations",
    "type": "library",
    "license": "MIT",
    "keywords": ["netserva", "automation", "cron", "scheduling", "tasks", "laravel", "filament"],
    "homepage": "https://netserva.org",
    "authors": [
        {
            "name": "Mark Constable",
            "email": "markc@netserva.org",
            "homepage": "https://netserva.org"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "filament/filament": "^4.0",
        "netserva/core": "*",
        "symfony/process": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NetServa\\Cron\\": "src/",
            "NetServa\\Cron\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetServa\\Cron\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetServa\\Cron\\NetServaCronServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}