{
    "name": "netserva/fleet",
    "description": "NetServa Fleet Management Plugin - VSite->VNode->VHost infrastructure tracking",
    "type": "library",
    "keywords": ["netserva", "fleet", "infrastructure", "ssh", "vm", "container"],
    "license": "MIT",
    "authors": [
        {
            "name": "NetServa Team",
            "email": "dev@netserva.org"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "filament/filament": "^4.0",
        "netserva/core": "*"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NetServa\\Fleet\\": "src/",
            "NetServa\\Fleet\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetServa\\Fleet\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetServa\\Fleet\\FleetServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}